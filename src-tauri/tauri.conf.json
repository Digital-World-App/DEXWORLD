{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dexworld-admin",
  "version": "0.1.0",
  "identifier": "com.dexworld",
  "build": {
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "dexworld-admin",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "http:default",
        "shell:open",
        "stronghold:default",
        "opener:open"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "http": {
      "scope": [
        "https://*.exemplo.com/api/*"
      ]
    },
    "shell": {},
    "stronghold": {},
    "opener": {}
  }
}